@model List<SharedObjects.ValueObjects.VSector>


<table class="table table-bordered table-hover table-striped ">
    <thead>
        <tr class="text-center bold font-green-sharp">
            <td>Sector</td>
            <td>Utilities</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr class="text-center">
                <td >@item.Sector</td>
                <td>
                    <a class="btn default btn-xs red remove" id="btn-delete" data-sector="@item.Sector" data-id="@item.Id" title="delete sector"> <span class="glyphicon glyphicon-minus"></span></a>
                </td>
            </tr>
        }
    </tbody>
</table>