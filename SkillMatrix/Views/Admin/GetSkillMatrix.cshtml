@model List<SharedObjects.ValueObjects.VSkillMatrix>


<table class="table table-bordered table-hover table-striped table-responsive">
    <thead>
        <tr class="text-center bold font-blue-sharp">

            <td>topic</td>
            <td>trainer</td>
            <td>weight</td>
            <td>evaluator</td>
            <td>evalScore</td>
            <td>assesScore</td>
            <td>Utilities</td>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                <span>@item.Topic</span>
            </td>
            <td>
                <span>@item.Trainer</span>
            </td>
            <td>
                <span>@item.Weight</span>

            </td>

            <td>
                <span>@item.Evaluator</span>
            </td>
            <td>
                <span>@item.EvalScore</span>
            </td>
            <td>
                <span>@item.AssesScore</span>
            </td>

            <td>
                <a class="btn default btn-xs red remove" data-username="@item.Trainer" title="Remove user"> <span class="glyphicon glyphicon-minus"></span></a>
            </td>
        </tr>
        }
    </tbody>
</table>